{
  "manifest_version": 3,
  "name": "SmarterTabSwitching",
  "version": "0.1",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "switch-tab": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Move to the first tab",
      "global": true
    }
  }
}
